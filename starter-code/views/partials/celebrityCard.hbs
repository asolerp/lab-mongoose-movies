<div class="celebrityCard">
  <div class="cardImage">
    <img src="{{this.img}}" alt="">
    <h1>{{this.name}}</h1>
  </div>
  <div class="cardButton"> 
    <a class='seeMoreButon' href="/celebrities/celebrity/{{this._id}}">See more</a>
    <a class="editButton" href="/celebrities/celebrity/{{this._id}}/edit">Edit</a>
